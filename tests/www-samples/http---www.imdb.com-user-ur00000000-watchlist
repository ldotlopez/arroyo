<script type="text/javascript" id="login">
(function(){
    var readyTimeout = setInterval(function() {
        if (window.jQuery && window.imdb && window.imdb.login_lightbox) {
            clearTimeout(readyTimeout);
            window.imdb.login_lightbox("https://secure.imdb.com", "http://www.imdb.com/user/ur00000000/watchlist");
        }
    }, 100);
})();
</script>

        <script type="text/javascript">
        function jQueryOnReady(remaining_count) {
            if (window.jQuery) {
                jQuery(
                         function() {
        // Init the watchlist rendering
        $("a.wlb_watchlist_lite").wlb_lite();
        $(".lister").watchlistRibbon(".ribbonize");
        
        $("a.watchlist-remove").on("click", function(event) {
            event.preventDefault();
            event.stopPropagation();
            $(this).parents(".lister-item").find(".ribbonize").trigger("do-remove-ribbon")
        });
        
        // Invoke the rating animation
        $(".lister-item").rating_animation();
        
        // TODO: we should have a method to populate all formated query params avaiable from mavAdvancedSearchForm
        // the sort param is required by CloudSearch, need to pass it if the current query link does not have it
        var queryString = window.location.search;
        // replaced the user sort inputs with sanitized sort ones
        var sortPattern = /[\\?&]sort=[^&#]*/g;
        queryString = queryString.replace(sortPattern, "");
        var sortQuery = 'list_order,asc';
        if (queryString.length == 0) {
            queryString = queryString + "?sort=" + sortQuery;
        } else {
            queryString = queryString + "&sort=" + sortQuery;
        }
        // ShowtimesDate is a hidden value from the 1st load, populate it here
        
        // lister initialization, returns an instance user for the faceter
        var listerInst = $('.lister').lister({
            externalCallbacks : [window.imdb.CS.filmosearchUpdate],
            initialMode : 'detail',
            initialPage : 1,
            initialParams : queryString,
            viewMoreOption : 'PAGINATE',
            ajaxRequestURL: ''
        });
        
        // Init faceter
        $(".faceter").faceter({
            Lister : listerInst.data('lister'),
            explore: ""
        });
        
        if ($('.lister-item.mode-grid').length) {
            // if grid mode, initialize the popouts
            $('.lister-item.mode-grid > .lister-item-image').smartPopout({
                containerSelector : '.lister',
                popoutSelector : '.lister-item-content',
                triangleAltitude : 9, // triangle altitude in px
                triangleBaseLength : 24 // triangle base length in px
            });
        }
     }

                );
                jQuery(
                             function() {
            // Add undoable action to the watchlist
            $(".lister").undoableRibbon(".ribbonize");
            
            /**
             * Wrapper for a mode change to update the Edit list link
             * @param {Event} e
             * @return void
             */
            var modeChangeUpdateEditLink = function(e) {
                var $clicked = e ? $(e.target) : {},
                    mode = $clicked.length ? $clicked.data('mode') : 'grid';
                updateEditLink(mode);
            };
            
            /**
             * Update the Edit list link depeding on what mode the user is viewing
             * @param {String} mode - the view mode
             * @return void
             */
            var updateEditLink = function(mode) {
                var $link = $('.rightcornerlink > .edit-link'),
                    href = $link.attr('href');
                if (mode === 'simple') {
                    // if simple mode and the Edit link's href is not compact, make it so
                    if (href !== 'watchlist?view=compact') {
                        $link.attr('href', 'watchlist?view=compact&ref_=wl_edt_pwr');
                    }
                } else {
                    // if not simple more and href is not list/edit, make it so
                    if (href !== '/list/edit?list_id=watchlist') {
                        $link.attr('href', '/list/edit?list_id=watchlist&ref_=wl_edt');
                    }
                }
            };
            
            // if there are simple items present then the user is in simple mode so the
            // edit button needs to be updated
            if ($('.lister-item.mode-simple').length) {
                updateEditLink('simple');
            }
            // if the user clicks a mode, the edit button needs to be updated accordingly
            $('.lister-mode').on('click', modeChangeUpdateEditLink);
        }

                );
                jQuery(
                           function() {
        "use strict";
        // Attach the event handler for faceter & trigger the click event again
        var listerInst = $('.lister').data('lister');
        if (typeof listerInst === 'undefined') {
            return;
        }
        
        var updateShowtimesFacetCount = function(e) {
            var showtimesAjaxURL = listerInst.getAjaxlink("now_playing", "now_playing"),
                $showtimesNode = $('div.faceter-fieldset.in_theaters fieldset[name="now_playing"]'),
                $tab = $('.faceter-category[data-for="in_theaters"]');
                
            // If we already get count for it, stop requesting again
            if (!$tab.attr('has-count')) {
                updateFacetCount(showtimesAjaxURL, $showtimesNode, $tab);
            }
       };
       
       var updateTVFacetCount = function(e) {
            var onTVAjaxURL = listerInst.getAjaxlink("on_tv", "on_tv"), 
                $onTVNode = $('div.faceter-fieldset.on_tv fieldset[name="on_tv"]'),
                $tab = $('.faceter-category[data-for="on_tv"]');
            
            // If we already get count for it, stop requesting again
            if (!$tab.attr('has-count')) { 
                updateFacetCount(onTVAjaxURL, $onTVNode, $tab);                
            }
        };
       
       var updateFacetCount = function(ajaxURL, node, tab) {
          $.ajax({
                url: ajaxURL,
                beforeSend: CS.addClickstreamHeadersToAjax,
                dataType: "html",
                type: "GET",
                success: function(data) {
                    if (typeof data !== 'undefined') {
                        $(node).replaceWith(data);
                        $(tab).attr('has-count', true);
                    }
                }, 
                error: function(jqXHR, textStatus, errorThrown) {
                    consoleLog('Unable to get the count :' + ajaxURL + "|" + errorThrown);
                }
            });
        };
        
        // Create a delegate event for showtimes field set
        $(".faceter").on("click",".faceter-category[data-for='in_theaters']", updateShowtimesFacetCount);
        // Create a delegate event for on tv field set
        $(".faceter").on("click", ".faceter-category[data-for='on_tv']", updateTVFacetCount);
                
        // make sure the current selected fieldset's click event is fired 
        // when the refiner is open
        if ($('.faceter').hasClass('open')) {
            $('div.faceter-categories .faceter-category.active').click();
        }
      }

                );
                jQuery(
                             function() {
           var isAdvertisingThemed = !!(window.custom && window.custom.full_page && window.custom.full_page.theme),
               url = "http://www.facebook.com/widgets/like.php?width=280&show_faces=1&layout=standard&href=http%3A%2F%2Fwww.imdb.com%2Fuser%2Fur00000000%2Fwatchlist&colorscheme=light",
               like = document.getElementById('iframe_like');

           if (!isAdvertisingThemed && like) {
              like.src = url;
              like.onload = function () { generic.monitoring.stop_timing('facebook_like_iframe', '', false); };
           } else if (isAdvertisingThemed) {
              $('.social_networking_like').closest('.aux-content-widget-2').hide();
           }
        }

                );
            } else if (remaining_count > 0) {
                setTimeout(function() { jQueryOnReady(remaining_count-1) }, 100);
            }
        }
        jQueryOnReady(50);
        </script>


<div id="servertime" time="212"/>



